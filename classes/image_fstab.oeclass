require conf/fstab.conf

IMAGE_PREPROCESS_FUNCS:>USE_fstab = " image_preprocess_fstab"

image_preprocess_fstab () {
	test -d ./${fstabfixupdir} || return 0
	(
	cwd=`pwd`
	cd .${fstabfixupdir}
	for f in * ; do
		echo -e "\n# $f" >> $cwd${sysconfdir}/fstab
		cat $f >> $cwd${sysconfdir}/fstab
	done
	)
	rm -rf .${fstabfixupdir}
}
